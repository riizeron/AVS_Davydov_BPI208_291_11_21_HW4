   119                                  ; Случайный ввод обобщенного числа
   120                                  ;----------------------------------------------
   121                                  global InRndNmber
   122                                  InRndNumber:
   123                                  section .data
   124 0000001C 52616E646F6D206E75-         .rndNumFmt       db "Random number = %d",10,0
   124 00000025 6D626572203D202564-
   124 0000002E 0A00               
   125                                  section .bss
   126 00000018 <res 00000008>              .pnum       resq    1   ; адрес числа
   127 00000020 <res 00000004>              .key        resd    1   ; ключ
   128                                  section .text
   129 000000B4 55                      push rbp
   130 000000B5 4889E5                  mov rbp, rsp
   131                                  
   132                                      ; В rdi адрес числа
   133 000000B8 48893C25[18000000]          mov [.pnum], rdi
   134                                  
   135                                      ; Формирование признака числа
   136 000000C0 4831C0                      xor     rax, rax    ;
   137 000000C3 E8(00000000)                call    rand        ; запуск генератора случайных чисел
   138 000000C8 83E001                      and     eax, 1      ; очистка результата кроме младшего разряда (0 или 1)
   139 000000CB FFC0                        inc     eax         ; фомирование признака числа (1 или 2 или 3)
   140                                  
   141                                      ;mov     [.key], eax
   142                                      ;mov     rdi, .rndNumFmt
   143                                      ;mov     esi, [.key]
   144                                      ;xor     rax, rax
   145                                      ;call    printf
   146                                      ;mov     eax, [.key]
   147                                  
   148 000000CD 488B3C25[18000000]          mov     rdi, [.pnum]
   149 000000D5 8907                        mov     [rdi], eax      ; запись ключа в фигуру
   150 000000D7 3B0425[00000000]            cmp eax, [COMPLEX]
   151 000000DE 7416                        je .compInRnd
   152 000000E0 3B0425[00000000]            cmp eax, [FRACTION]
   153 000000E7 741D                        je .fracInRnd
   154 000000E9 3B0425[00000000]            cmp eax, [POLAR]
   155 000000F0 7424                        je .polInRnd
   156 000000F2 31C0                        xor eax, eax        ; код возврата = 0
   157 000000F4 EB2E                        jmp     .return
   158                                  .compInRnd:
   159                                      ; Генерация комплексного числав
   160 000000F6 4883C704                    add     rdi, 4
   161 000000FA E820FFFFFF                  call    InRndComplex
   162 000000FF B801000000                  mov     eax, 1      ;код возврата = 1
   163 00000104 EB1E                        jmp     .return
   164                                  .fracInRnd:
   165                                      ; Генерация дроби
   166 00000106 4883C704                    add     rdi, 4
   167 0000010A E83DFFFFFF                  call    InRndFraction
   168 0000010F B801000000                  mov     eax, 1      ;код возврата = 1
   169 00000114 EB0E                        jmp     .return
   170                                  .polInRnd:
   171                                      ; Генерация полярной координаты
   172 00000116 4883C704                    add     rdi, 4
   173 0000011A E861FFFFFF                  call    InRndPolar
   174 0000011F B801000000                  mov     eax, 1      ;код возврата = 1
   175                                  .return:
   176 00000124 C9                      leave
   177 00000125 C3                      ret
