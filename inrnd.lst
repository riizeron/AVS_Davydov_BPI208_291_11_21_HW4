   209 00000155 5A                          pop rdx
   210 00000156 5B                          pop rbx
   211 00000157 48FFC3                      inc rbx
   212                                  
   213 0000015A 5F                          pop rdi
   214 0000015B 4883C710                    add rdi, 16             ; адрес следующего числа
   215                                  
   216 0000015F EBE2                        jmp .loop
   217                                  .return:
   218 00000161 488B0425[2C000000]          mov rax, [.plen]    ; перенос указателя на длину
   219 00000169 8918                        mov [rax], ebx      ; занесение длины
   220 0000016B C9                      leave
   221 0000016C C3                      ret
